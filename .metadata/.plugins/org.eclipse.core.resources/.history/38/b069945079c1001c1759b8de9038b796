package com.perscholas.monster;

public class FireMonster extends Monster {
	private String name;
	
	public FireMonster(String name) {
		// TODO Auto-generated constructor stub
		super(name);
	}
	
	
	
	String attack() {
		if(this.name != "u2u2") {
		 return this.name + "attack";
		}
		else {
			return "garbage";
		}
	}

}
