package com.perscholas.java_basics;

public class JavaBasicsClass {

	public JavaBasicsClass() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		//one
		int  a = 1;
		int b = 2;
		int sum = a + b ;
		System.out.println(sum);
		//two
		double  c = 123412.4;
		double d = 254855.5;
		double sum_double = a + b ;
		System.out.println(sum_double);
		//third
		int  e = 1234;
		double f = 254855.5;
		double sum_int_double = a + b ; // sum should be double
		System.out.println(sum_int_double);
		//four 
		int  small = 1;
		int large = 2;
		int divid_by_small =  b/a ;
		System.out.println(divid_by_small);
		float divid_by_small_float =  (float) b / a ;
		System.out.println(divid_by_small);
		
		
		

	}

}
