package firstjavaclass;

public class Welcome {

	public Welcome() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// Datatypes basics
		byte b = 10;
		short s = 123;
		int num = 345678912;
		long lnum = num * 4;
		float fnum = lnum * 5;
		double dnum = 5212*lnum;
		char onechar = 'C';
		String word = "Here we are great";
		
		System.out.println(word + num + onechar);
	}

}
