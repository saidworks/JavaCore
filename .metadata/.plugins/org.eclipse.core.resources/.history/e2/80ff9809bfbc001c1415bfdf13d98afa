package com.perscholas.java_basics;

public class Conditionals {

	public Conditionals() {
		// TODO Auto-generated constructor stub
	}
	public static void main(String[] args) {
		//Q1
		int x;
		x = 7;
		if(x<10) {
			System.out.println("Less than 10");
		}
		else {
			System.out.println("bigger than 10");
		}
	}
}
