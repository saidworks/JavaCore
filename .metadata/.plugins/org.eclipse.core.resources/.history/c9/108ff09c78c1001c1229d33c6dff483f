package com.perscholas.monster;

public class Monster {
	private String name;
	public Monster() {
		// TODO Auto-generated constructor stub
	}

	public Monster(String name) {
		// TODO Auto-generated constructor stub
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	String attack() {
		return this.name+"'s attack";
	}
	
}
